con Align:\" name=\"alignIcon\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"id\" name=\"CardId\">\n        <header>Color</header>\n        <input type=\"color\" title=\"Color Card Front:\" name=\"colorCard\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Color Card Back:\" name=\"colorCard2\" value=\"#f4f4f4\">\n        <input type=\"color\" title=\"Color Icon:\" name=\"colorIcon\" value=\"#7e7e7e\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Color Icon-2:\" name=\"colorIcon2\" value=\"#4285f4\" condition=\"showIcon2\">\n        <!-- <input type=\"checkbox\" title=\"Border:\" name=\"borderCard\"> -->\n        <!--  -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#ffffff\">\n            <input type=\"image\" title=\"Background Image:\" value=\"@PROJECT_PATH@/assets/images/bike.jpg\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <!-- Title -->\n        <div class=\"title-block\">\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-regular mbr-white\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">CREATED NO